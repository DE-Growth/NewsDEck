# 네이버 뉴스 API 설정
NAVER_CLIENT_ID=your_naver_client_id_here
NAVER_CLIENT_SECRET=your_naver_client_secret_here

# Kafka 설정
KAFKA_BOOTSTRAP_SERVERS=kafka:9092
KAFKA_TOPIC=news-topic

# 뉴스 수집 설정
COLLECT_KEYWORDS=IT,주식,AI,반도체,경제
COLLECT_INTERVAL=3600
ARTICLES_PER_KEYWORD=10
RUN_MODE=continuous

# 데이터베이스 설정
DB_HOST=postgres
DB_NAME=airflow
DB_USER=airflow
DB_PASSWORD=airflow
DB_PORT=5432

# 로깅 설정
LOG_LEVEL=INFO
